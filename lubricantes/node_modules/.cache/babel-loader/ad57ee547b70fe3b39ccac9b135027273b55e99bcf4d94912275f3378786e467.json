{"ast":null,"code":"import axios from 'axios';\nimport { confirmar } from '../../funciones';\nexport default {\n  data() {\n    const api = process.env.VUE_APP_API;\n    return {\n      api,\n      activo: null\n    };\n  },\n  mounted() {\n    this.getActivo();\n  },\n  methods: {\n    getActivo() {\n      console.log(\"api\" + this.api);\n      axios.get(this.api + '/Activos').then(response => this.activo = response.data);\n    },\n    eliminar(id, nombre) {\n      confirmar(id, nombre, this.api + '/Activos/', 1);\n    }\n  }\n};","map":{"version":3,"names":["axios","confirmar","data","api","process","env","VUE_APP_API","activo","mounted","getActivo","methods","console","log","get","then","response","eliminar","id","nombre"],"sources":["/home/xavi/Documentos/Dilpmado Full/mod 6 Desarrollo Frontend con VueJS/final/lubricantes/src/views/activo/HomeActivo.vue"],"sourcesContent":["<template>\n    <div class=\"row\">\n        <div class=\"col-lg-8 offset-lg-2\">\n            <div class=\"table-responsive\">\n                <table class=\"table table-bordered table-hover\">\n                    <thead><tr><th>#</th><th>NOMBRE</th><th>DESCRIPCION</th><th>COLOR</th><th>COSTO</th><th>ACCIONES</th></tr></thead>\n                        <tbody class=\"table-group-divider\" id=\"contenido\">\n                            <tr v-for=\"acti, i in activo\" :key=\"acti.id\">\n                            <td>{{ (i + 1) }}</td>\n                            <td>{{ acti.nombre }}</td>\n                            <td>{{ acti.descripcion }}</td>\n                            <td>{{ acti.color }}</td>\n                            <td>${{new Intl.NumberFormat('es-bo').format(acti.costo) }}</td>\n                            <td>\n                                <router-link :to=\"{path:'editaractivo/'+acti.id}\" class=\"btn btn-warning\">\n                                    <i class=\"fa-solid fa-edit\"></i>\n                                </router-link> &nbsp;\n                                <button class=\"btn btn-danger\" v-on:click=\"eliminar(acti.id,acti.nombre)\">\n                                    <i class=\"fa-solid fa-trash\"></i>\n                                </button>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\n    import axios from 'axios';\n    import { confirmar } from '../../funciones'\n    export default{\n        data(){\n            const api = process.env.VUE_APP_API;\n            return{ api,\n                    activo:null}\n        },\n        mounted(){\n            this.getActivo();\n        },\n        methods: {\n            getActivo(){\n                console.log(\"api\"+this.api)\n                axios.get(this.api+'/Activos').then(\n                    response => (\n                        this.activo = response.data\n                    )\n                );\n            },\n            eliminar(id,nombre){\n                confirmar(id,nombre,this.api+'/Activos/',1);\n            }\n        }\n    }\n</script>"],"mappings":"AA6BI,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,iBAAgB;AAC1C,eAAc;EACVC,IAAIA,CAAA,EAAE;IACF,MAAMC,GAAE,GAAIC,OAAO,CAACC,GAAG,CAACC,WAAW;IACnC,OAAM;MAAEH,GAAG;MACHI,MAAM,EAAC;IAAI;EACvB,CAAC;EACDC,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,SAAS,CAAC,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACLD,SAASA,CAAA,EAAE;MACPE,OAAO,CAACC,GAAG,CAAC,KAAK,GAAC,IAAI,CAACT,GAAG;MAC1BH,KAAK,CAACa,GAAG,CAAC,IAAI,CAACV,GAAG,GAAC,UAAU,CAAC,CAACW,IAAI,CAC/BC,QAAO,IACH,IAAI,CAACR,MAAK,GAAIQ,QAAQ,CAACb,IAE/B,CAAC;IACL,CAAC;IACDc,QAAQA,CAACC,EAAE,EAACC,MAAM,EAAC;MACfjB,SAAS,CAACgB,EAAE,EAACC,MAAM,EAAC,IAAI,CAACf,GAAG,GAAC,WAAW,EAAC,CAAC,CAAC;IAC/C;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}